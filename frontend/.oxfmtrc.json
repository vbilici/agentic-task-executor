{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "experimentalSortImports": {
    "groups": [
      ["builtin", "external"],
      ["internal", "parent", "sibling", "index"]
    ],
    "newlinesBetween": true,
    "ignoreCase": true
  },
  "ignorePatterns": [
    "**/build",
    "**/public",
    "pnpm-lock.yaml",
    "**/*.gen.ts*"
  ]
}
